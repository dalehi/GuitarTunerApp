<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jonathanturnbull.guitarturnerapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/tunericon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/tunericon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginPage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".AddRecordPage" />
        <activity android:name=".TunerPage" />
        <activity android:name=".RecordPage" />
        <activity android:name=".LyricsPage" />
        <activity android:name=".MetronomePage" />
        <activity android:name=".RegisterPage" />
        <activity android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>

            </intent-filter>

        </activity>
    </application>

</manifest>